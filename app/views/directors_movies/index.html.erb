<p id="notice"><%= notice %></p>

<h1>Directors Movies</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @directors_movies.each do |directors_movie| %>
      <tr>
        <td><%= link_to 'Show', directors_movie %></td>
        <td><%= link_to 'Edit', edit_directors_movie_path(directors_movie) %></td>
        <td><%= link_to 'Destroy', directors_movie, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Directors Movie', new_directors_movie_path %>
